{
  "type": "object",
  "properties": {
    "user_question": {
      "type": "string",
      "description": "The original question or request from the user"
    },
    "analytics_data": {
      "type": "object",
      "properties": {
        "total_queries": {"type": "integer"},
        "successful_queries": {"type": "integer"},
        "comprehensive_insights": {
          "type": "array",
          "items": {"type": "string"}
        },
        "query_results": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "query": {"type": "string"},
              "purpose": {"type": "string"},
              "result": {
                "type": "object",
                "properties": {
                  "success": {"type": "boolean"},
                  "rows": {"type": "array"},
                  "columns": {"type": "array"},
                  "row_count": {"type": "integer"}
                }
              }
            }
          }
        }
      }
    },
    "question_answering_response": {
      "type": "string",
      "description": "The comprehensive answer generated by the question answering agent"
    },
    "file_summaries": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "table_name": {"type": "string"},
          "original_file_name": {"type": "string"},
          "file_summary": {
            "type": "object",
            "properties": {
              "overview": {"type": "string"},
              "business_domain": {"type": "string"},
              "data_structure": {"type": "object"}
            }
          }
        }
      }
    },
    "analysis_context": {
      "type": "object",
      "properties": {
        "analysis_depth": {"type": "string"},
        "data_sources_used": {
          "type": "array",
          "items": {"type": "string"}
        },
        "research_completeness": {"type": "string"}
      }
    },
    "research_plan": {
      "type": "string",
      "description": "The research plan and analysis approach used"
    }
  },
  "required": ["user_question", "analytics_data", "question_answering_response"]
} 