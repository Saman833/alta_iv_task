{
  "type": "object",
  "properties": {
    "user_question": {
      "type": "string",
      "description": "The original question from the user that needs to be answered"
    },
    "analytics_results": {
      "type": "object",
      "properties": {
        "total_queries_executed": {"type": "integer"},
        "successful_queries": {"type": "integer"},
        "query_results": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "query": {"type": "string"},
              "purpose": {"type": "string"},
              "result": {
                "type": "object",
                "properties": {
                  "success": {"type": "boolean"},
                  "rows": {"type": "array"},
                  "columns": {"type": "array"},
                  "row_count": {"type": "integer"},
                  "data_summary": {"type": "string"}
                }
              }
            }
          }
        },
        "key_insights": {
          "type": "array",
          "items": {"type": "string"}
        },
        "data_quality_metrics": {
          "type": "object",
          "properties": {
            "total_records_analyzed": {"type": "integer"},
            "data_completeness": {"type": "string"},
            "confidence_level": {"type": "string"}
          }
        }
      }
    },
    "question_answering_response": {
      "type": "string",
      "description": "The comprehensive answer generated by the question answering agent"
    },
    "research_plan": {
      "type": "string",
      "description": "The research approach and methodology used"
    },
    "file_summaries": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "table_name": {"type": "string"},
          "original_file_name": {"type": "string"},
          "file_summary": {
            "type": "object",
            "properties": {
              "overview": {"type": "string"},
              "business_domain": {"type": "string"},
              "data_structure": {"type": "object"}
            }
          }
        }
      }
    }
  },
  "required": ["user_question", "analytics_results", "question_answering_response"]
} 